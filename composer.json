{
    "name": "paliari/doctrine-ransack",
    "version": "1.2.0",
    "description": "Advanced query searching for Doctrine ORM (based on Ruby on Rails Ransack)",
    "type": "library",
    "require": {
        "php": ">=7.3",
        "doctrine/orm": "^2.6"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Marcos Paliari",
            "email": "marcos@paliari.com.br"
        }
    ],
    "homepage": "https://github.com/paliari/doctrine-validator",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/paliari/doctrine-ransack.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Paliari\\Doctrine\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "paliari/php-utils": "^1.1"
    }
}